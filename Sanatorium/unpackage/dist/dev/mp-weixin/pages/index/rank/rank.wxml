<view class="page"><block wx:for="{{rankList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{index<3}}"><view data-event-opts="{{[['tap',[['seedetail',[index]]]]]}}" class="{{[(index==0)?'top1':'',(index==1)?'top2':'',(index==2)?'top3':'']}}" bindtap="__e"><block wx:if="{{index==0}}"><view><image class="crown" src="../../../static/images/crown.png"></image></view></block><block wx:if="{{index!=0}}"><view>{{index+1}}</view></block><view><image class="rankimg" src="{{item.picUrl}}"></image></view><view class="rankname">{{item.candidateName}}</view><view class="rankticket">{{item.voteNum+"票"}}</view></view></block></block><view class="line2"></view><block wx:for="{{rankList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{index>3}}"><view data-event-opts="{{[['tap',[['seedetail',[index]]]]]}}" class="otherrank" bindtap="__e"><view class="rankid">{{index}}</view><view><image class="rankimg" src="{{item.picUrl}}"></image></view><view class="rankname">{{item.candidateName}}</view><view class="rankticket">{{item.voteNum+"票"}}</view></view></block></block></view>